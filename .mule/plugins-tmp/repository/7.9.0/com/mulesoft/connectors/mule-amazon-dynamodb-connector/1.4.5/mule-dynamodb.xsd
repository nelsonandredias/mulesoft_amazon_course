  <xs:element xmlns="http://www.mulesoft.org/schema/mule/dynamodb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="BatchPutItemType" substitutionGroup="mule:abstract-operator" name="batch-put-item">
