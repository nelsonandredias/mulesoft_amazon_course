  <xs:element xmlns="http://www.mulesoft.org/schema/mule/dynamodb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CreateTableType" substitutionGroup="mule:abstract-operator" name="create-table">
    <xs:annotation>
      <xs:documentation>The CreateTable operation adds a new table to your account. In an AWS account, table names must be unique within each region.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CreateTableType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
