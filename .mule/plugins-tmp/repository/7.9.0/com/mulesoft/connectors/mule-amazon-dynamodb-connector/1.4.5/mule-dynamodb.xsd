          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="attribute-definitions">
            <xs:annotation>
              <xs:documentation>An array of attributes that describe the key schema for the table and indexes.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element xmlns:dynamodb="http://www.mulesoft.org/schema/mule/dynamodb" minOccurs="0" maxOccurs="unbounded" ref="dynamodb:abstract-attribute-definition"></xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="key-schemas">
            <xs:annotation>
              <xs:documentation>Specifies the attributes that make up the primary key for a table or an index.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element xmlns:dynamodb="http://www.mulesoft.org/schema/mule/dynamodb" minOccurs="0" maxOccurs="unbounded" ref="dynamodb:abstract-key-schema-element"></xs:element>
